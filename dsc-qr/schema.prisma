// Modify your Keystone config when you want to change this.

datasource sqlite {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "sqlite"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String    @id @default(cuid())
  name      String    @unique @default("")
  password  String
  createdAt DateTime? @default(now())
}

model Survey {
  id          String       @id @default(cuid())
  form        String?
  submissions Submission[] @relation("Submission_survey")
}

model Submission {
  id        String    @id @default(cuid())
  survey    Survey?   @relation("Submission_survey", fields: [surveyId], references: [id])
  surveyId  String?   @map("survey")
  response  String?
  createdAt DateTime? @default(now())

  @@index([surveyId])
}
